{
  "name": "@auvred/eslint-config",
  "version": "0.1.0",
  "packageManager": "pnpm@8.6.3",
  "author": "auvred <aauvred@gmail.com> (https://github.com/auvred/)",
  "description": "⚙️ Opinionated ESLint config for Typescript and Vue projects",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "homepage": "https://github.com/auvred/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/auvred/eslint-config"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint .",
    "postinstall": "simple-git-hooks",
    "release": "bumpp && pnpm publish"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,json,yml,yaml,md}": "eslint --fix"
  },
  "dependencies": {
    "@so1ve/eslint-plugin-prettier": "^0.106.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "eslint-plugin-yml": "^1.8.0",
    "fast-glob": "^3.2.12",
    "find-up": "^5.0.0",
    "js-yaml": "^4.1.0",
    "jsonc-eslint-parser": "^2.3.0",
    "prettier-plugin-jsdoc": "^0.4.2"
  },
  "devDependencies": {
    "bumpp": "^9.1.1",
    "eslint": "^8.43.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "prettier": ">=2.0.0"
  }
}
